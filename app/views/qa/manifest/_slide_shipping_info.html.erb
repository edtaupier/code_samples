<table id="qaManifestTable">
  <thead>
    <tr>
      <th width="11%">
        First Name
      </th>
      <th width="11%">
        Last Name
      </th>
      <th width="20%">
        Address Shipped To
      </th>
      <th width="15%">
        Secondary Address
      </th>
      <th width="12%">
        City
      </th>
      <th width="4%">
        ST
      </th>
      <th width="6%">
        Zip
      </th>
      <th width="12%">
        Phone Number
      </th>
      <th width="12%">
        Fax Number
      </th>

    </tr>
  </thead>
  <tbody>
        <tr>
          <td><%= shipping.contactFirstName %></td>
          <td><%= shipping.contactLastName %></td>
          <td><%= shipping.addressLineOne %></td>
          <td><%= shipping.addressLineTwo %></td>
          <td><%= shipping.city %></td>
          <td><%= shipping.state %></td>
          <td><%= shipping.zip %></td>
          <td><%= shipping.phoneNumber.phone_format %></td>
          <td><%= shipping.faxNumber.phone_format %></td>
        </tr>
  </tbody>
</table>
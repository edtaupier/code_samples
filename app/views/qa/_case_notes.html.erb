<div id="caseNotesContainer" class="panel bordered-box">
    <div class="ui-widget-header">
        <%= image_tag "Medical_Invoice_Warning.gif" %>
        <div>Case Notes</div>
    </div>

    <div id="caseNotes">
        <table id="caseNotesTable">
          <thead>
            <tr>
              <th style="width:30%">
                Posted On
              </th>
              <th style="width:50%">
                Subject
              </th>
              <th style="width:20%">
                User
              </th>
            </tr>
          </thead>
          <tbody>
            <% notes.each do |note| %>
                <%= render "qa/case_note_row", :note=>note %>
            <% end %>
          </tbody>
        </table>
    </div>

</div>